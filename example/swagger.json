    "host": "peppa-agent-manage.qa.huohua.cn",
    "basePath": "/",
    "tags": [{
        "name": "代理人接口",
        "description": "Agent Feign Controller"
    }, {
        "name": "商品banner接口",
        "description": "Banner Controller"
    }],
    "paths": {
        "/server/agent/getByUserId": {
            "get": {
                "description": "通过用户ID获取代理人信息",
                "tags": ["代理人接口"],
                "summary": "根据userId查询代理人",
                "operationId": "queryAgentByUserIdUsingGET",
                "consumes": ["application/json"],
                "produces": ["*/*"],
                "parameters": [{
                    "name": "userId",
                    "in": "query",
                    "description": "用户ID",
                    "required": true,
                    "type": "integer",
                    "format": "int32"
                }],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/AgentVo"
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "403": {
                        "description": "Forbidden"
                    },
                    "404": {
                        "description": "Not Found"
                    }
                }
            }
        },
        "/server/agent/listByUserIds": {
            "get": {
                "tags": ["代理人接口"],
                "summary": "通过用户批量获取代理人",
                "operationId": "listByUserIdsUsingGET",
                "consumes": ["application/json"],
                "produces": ["*/*"],
                "parameters": [{
                    "in": "body",
                    "name": "userIds",
                    "description": "userIds",
                    "required": true,
                    "schema": {
                        "type": "array",
                        "items": {
                            "type": "integer",
                            "format": "int32"
                        }
                    }
                }],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/AgentVo"
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "403": {
                        "description": "Forbidden"
                    },
                    "404": {
                        "description": "Not Found"
                    }
                }
            }
        },
        "/server/user/banner/getBanners": {
            "get": {
                "tags": ["商品banner接口"],
                "summary": "获取banner图地址集合",
                "operationId": "getBannersUsingGET",
                "consumes": ["application/json"],
                "produces": ["*/*"],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized"
                    },
                    "403": {
                        "description": "Forbidden"
                    },
                    "404": {
                        "description": "Not Found"
                    }
                }
            }
        }
    },
    "definitions": {
        "AgentVo": {
            "type": "object",
            "properties": {
                "employeeId": {
                    "type": "integer",
                    "format": "int32",
                    "description": "运营人员ID"
                },
                "employeeName": {
                    "type": "string",
                    "description": "运营人员名字"
                },
                "hidePhone": {
                    "type": "string",
                    "description": "隐藏后的手机号"
                },
                "id": {
                    "type": "integer",
                    "format": "int32",
                    "description": "ID"
                },
                "name": {
                    "type": "string",
                    "description": "名"
                },
                "nickName": {
                    "type": "string",
                    "description": "昵称"
                },
                "parentAgentId": {
                    "type": "integer",
                    "format": "int32",
                    "description": "上级代理人ID"
                },
                "profilePhotoUrl": {
                    "type": "string",
                    "description": "代理人头像"
                },
                "registeredTime": {
                    "type": "string",
                    "format": "date-time",
                    "description": "注册时间"
                },
                "status": {
                    "type": "integer",
                    "format": "int32",
                    "description": "状态: 0-禁用, 1-启用"
                }
            },
            "description": "代理人"
        }
    }
